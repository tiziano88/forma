{
  "name": "lintx-structural-editor",
  "displayName": "Lintx Structural Editor",
  "description": "View and edit Protobuf-encoded binary files with a schema using a Svelte-based webview.",
  "version": "0.0.1",
  "publisher": "your-publisher",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onCommand:lintx.openStructuralEditor",
    "onCommand:lintx.openForActiveFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lintx.openStructuralEditor",
        "title": "Open Structural Editor"
      },
      {
        "command": "lintx.openForActiveFile",
        "title": "Open Structural Editor for Active File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "build": "tsc -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.4.0",
    "@types/node": "^20.0.0"
  }
}
